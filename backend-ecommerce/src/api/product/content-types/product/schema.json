{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "biginteger"
    },
    "categoryObject": {
      "type": "enumeration",
      "enum": [
        "collares",
        "correas",
        "jugetes",
        "ropa",
        "camas",
        "bebederos"
      ]
    },
    "stock": {
      "type": "integer"
    },
    "isFeature": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::article.article"
    }
  }
}
